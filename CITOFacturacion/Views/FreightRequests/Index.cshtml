@model IEnumerable<CITOFacturacion.Models.Entities.FreightRequest>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Restrictions)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReqClientRate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreateDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Restrictions)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReqClientRate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreateDate)
